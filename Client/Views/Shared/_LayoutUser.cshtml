                    </div><!-- #logo end -->

                    <nav id="primary-menu">
                        <ul>
                            <li>
                                <a href="/Login"><div>Login</div></a>
                            </li>
                            <li>
                                <a href="/Register"><div>Register</div></a>
                            </li>
                        </ul>
                    </nav><!-- #primary-menu end -->

                </div>
            </div>

        </header><!-- #header end -->
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <footer>
            <div id="copyrights">

                <div class="container clearfix">

                    <div class="col_half">
